{
  "expo": {
    "name": "chattr",
    "slug": "chattr",
    "version": "1.0.0",
    "scheme": "chattr",
    "orientation": "portrait",
    "icon": "./assets/Chattr-logo/profile.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Chattr-logo/profile.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
